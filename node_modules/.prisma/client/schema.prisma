generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Customer {
  id        Int      @id @default(autoincrement())
  username  String
  password  String
  email     String?  @unique
  name      String
  sex       SexEnum
  address   String
  phone     String
  birthdate DateTime
}

enum SexEnum {
  MALE
  FEMALE
  PREFER_NOT_TO_SAY
}
